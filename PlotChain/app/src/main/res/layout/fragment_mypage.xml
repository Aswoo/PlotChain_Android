<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/black"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></View>
    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="마이페이지"
        android:textSize="20sp"
        android:textColor="@color/white"
        android:fontFamily="@font/nanumsquareotfregular"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/myinfo_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4"></View>


    <TextView
        android:id="@+id/tv_mypage_info"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="내정보"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/myinfo_view"
        app:layout_constraintLeft_toLeftOf="@id/myinfo_view"
        app:layout_constraintTop_toTopOf="@id/myinfo_view" />


    <View
        android:id="@+id/mycomment_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myinfo_view"></View>


    <TextView
        android:id="@+id/tv_mypage_comment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="16dp"
        android:text="내가 남긴 댓글"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/mycomment_view"
        app:layout_constraintLeft_toLeftOf="@id/mycomment_view"
        app:layout_constraintTop_toTopOf="@id/mycomment_view" />


    <View
        android:id="@+id/setting_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/mycomment_view"></View>


    <TextView
        android:id="@+id/tv_mypage_setting"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="16dp"
        android:text="설정"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/setting_view"
        app:layout_constraintLeft_toLeftOf="@id/setting_view"
        app:layout_constraintTop_toTopOf="@id/setting_view" />


    <View
        android:id="@+id/notice_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/setting_view"></View>

    <TextView
        android:id="@+id/tv_mypage_notice"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="16dp"
        android:text="공지사항"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/notice_view"
        app:layout_constraintLeft_toLeftOf="@id/notice_view"
        app:layout_constraintTop_toTopOf="@id/notice_view" />


    <View
        android:id="@+id/service_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notice_view"></View>

    <TextView
        android:id="@+id/tv_mypage_service"
        android:layout_width="0dp"
        android:layout_height="wrap_content"


        android:layout_marginLeft="16dp"
        android:text="고객센터"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/service_view"
        app:layout_constraintLeft_toLeftOf="@id/service_view"
        app:layout_constraintTop_toTopOf="@id/service_view" />

    <View
        android:id="@+id/invide_view"
        android:layout_width="match_parent"
        android:layout_height="0.8dp"
        android:layout_marginTop="24dp"
        android:background="@color/gray_view_color"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/service_view" />


    <View
        android:id="@+id/writer_view"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invide_view"></View>


    <TextView
        android:id="@+id/tv_mypage_writer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"

        android:layout_marginLeft="16dp"
        android:text="작가 페이지"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/writer_view"
        app:layout_constraintLeft_toLeftOf="@id/writer_view"
        app:layout_constraintTop_toTopOf="@id/writer_view" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fb_writepage_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="24dp"
        app:fabSize="normal"

        android:backgroundTint="@color/black"
        android:clickable="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_mypage_pencil" />


</android.support.constraint.ConstraintLayout>